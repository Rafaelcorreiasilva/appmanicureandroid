<AjustesPage>:
    FloatLayout:
        Label:
            canvas.before:
                Rectangle:
                    pos: self.pos
                    size: self.size
                    source: "icones/botao_fino.png"

            pos_hint: {"right": 0.85 , "top": 0.95}
            size_hint: 0.7, 0.2
            bold: True
            text: "Seu ID Único: 1"
            id: id_profissional

        # Botão voltar
        FloatLayout:
            pos_hint: {"right": 1 , "top": 0.12}
            size_hint: 1, 0.10
            canvas:
                Color:
                    rgb: utils.get_color_from_hex("#006D7E")
                Rectangle:
                    size: self.size
                    pos: self.pos
            ImageButton:
                pos_hint: {"right": 0.2, "top": 0.9}
                size_hint: 0.20, 0.8
                source: "icones/voltar.png"
                on_release: app.mudar_tela("inicialpage")
